<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="default_btn_width">150dp</dimen>
    <dimen name="default_btn_height">150dp</dimen>

    <declare-styleable name="FunctionBtnView">
        <attr name="function_icon" format="reference" />
        <attr name="function_name" format="string" />
        <attr name="imgBtn_width" format="dimension" />
        <attr name="imgBtn_height" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FileProgressTagView">
        <attr name="file_name" format="string" />
        <attr name="file_size" format="string" />
    </declare-styleable>

    <declare-styleable name="DeviceTagView">
        <attr name="device_name" format="string" />
        <attr name="device_ip" format="string" />
    </declare-styleable>
</resources>